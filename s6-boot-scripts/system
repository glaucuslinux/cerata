install() {
  $RSYNC $SRCD/$nom/etc $VNMD/$nom/sac --delete
  $RSYNC $SRCD/$nom/usr $VNMD/$nom/sac --delete

  #s6-rc-compile $VNMD/$nom/sac/etc/s6/rc/compiled $VNMD/$nom/sac/etc/s6/sv
}
