install() {
  $MKDIR $VNMD/$nom/sac/etc
  $RSYNC $SRCD/$nom/$nom-$ver/protocols $VNMD/$nom/sac/etc
  $RSYNC $SRCD/$nom/$nom-$ver/services $VNMD/$nom/sac/etc
}
