prepare() {
  cd $TBLD/gcc
}

configure() {
  :
}

build() {
  $MAKE \
    all-target-$nom
}

check() {
  :
}

install() {
  $MAKE \
    install-strip-target-$nom

  # Remove libtool archive (.la) file
  $RM $TLCD/usr/lib/libgomp.la
}
