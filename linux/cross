ceras_install() {
  rsync $RADULA_RSYNC_FLAGS /boot/vmlinuz-linux $CRSD/boot/vmlinuz

  rsync $RADULA_RSYNC_FLAGS /usr/lib/modules $CRSD/usr/lib
}
