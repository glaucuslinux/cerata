prepare() {
  $MKDIR $XBLD/$nom
  cd $XBLD/$nom
}

configure() {
  :
}

build() {
  sh $SRCD/$nom/$nom/Build.sh \
    -r
}

check() {
  :
}

install() {
  $RSYNC $nom $CRSD/usr/bin
  $LN $nom $CRSD/usr/bin/ksh
  $LN $nom $CRSD/usr/bin/rksh
}
