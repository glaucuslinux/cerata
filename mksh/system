prepare() {
  $MKDIR $SBLD/$nom
  cd $SBLD/$nom
}

configure() {
  :
}

build() {
  sh $SRCD/$nom/$nom/Build.sh \
    -r
}

check() {
  :
}

install() {
  $RSYNC $nom $SCER/$nom/sac/usr/bin
  $LN $nom $SCER/$nom/sac/usr/bin/ksh
  $LN $nom $SCER/$nom/sac/usr/bin/rksh
}
